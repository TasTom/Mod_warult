<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<QuestScriptDef>
		<defName>Expedition33_ArtifactRecovery</defName>
		<label>Récupération d'Artefact</label>
		<description>Une expédition a découvert un artefact mystérieux qui pourrait révéler des secrets sur Le Gommage.</description>
		<rootSelectionWeight>1.0</rootSelectionWeight>
		<canGiveRoyalFavor>false</canGiveRoyalFavor>
		<expireDaysRange>15~25</expireDaysRange>
		<root Class="QuestNode_Sequence">
			<nodes>
				<li Class="QuestNode_SubScript">
					<def>Util_GenerateSite</def>
				</li>
				<li Class="QuestNode_SpawnWorldObjects">
					<worldObjects>
						<li Class="WorldObjectDef">
							<defName>Site_ExpeditionArtifact</defName>
						</li>
					</worldObjects>
				</li>
			</nodes>
		</root>
	</QuestScriptDef>
</Defs>