<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <!-- =============================================================== -->
  <!--               RACE DE BASE (abstraite)                          -->
  <!-- =============================================================== -->
  <ThingDef Name="Expedition33_BaseBossRace" ParentName="BasePawn" Abstract="True">
    <comps>
      <li Class="Mod_warult.CompProperties_BossAggro">
        <aggroRadius>80</aggroRadius>
        <alwaysHostile>true</alwaysHostile>
      </li>
      <li Class="Mod_warult.CompProperties_AutoHealBleed"/>
      <li Class="Mod_warult.CompProperties_NoNeeds"/>
    </comps>
    
    <statBases>
      <MoveSpeed>5.0</MoveSpeed>
      <MarketValue>500</MarketValue>
      <ComfyTemperatureMin>-100</ComfyTemperatureMin>
      <ComfyTemperatureMax>100</ComfyTemperatureMax>
      <LeatherAmount>0</LeatherAmount>
      <MeatAmount>0</MeatAmount>
    </statBases>
    
    <race>
      <renderTree>Humanlike</renderTree>
      <thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
      <intelligence>Humanlike</intelligence>
      <makesFootprints>true</makesFootprints>
      <lifeExpectancy>200</lifeExpectancy>
      <bloodDef>Filth_Blood</bloodDef>
      <bloodSmearDef>Filth_BloodSmear</bloodSmearDef>
      <leatherDef>Leather_Human</leatherDef>
      <fleshType>Normal</fleshType>
      <hasGenders>true</hasGenders>
      <nameCategory>HumanStandard</nameCategory>
      <body>Human</body>
      <meatColor>(102, 51, 76)</meatColor>
      <baseHealthScale>1.2</baseHealthScale>
      <baseBodySize>1</baseBodySize>
      <foodType>OmnivoreHuman</foodType>
      <gestationPeriodDays>45</gestationPeriodDays>
      <manhunterOnDamageChance>0.20</manhunterOnDamageChance>
      <manhunterOnTameFailChance>0.02</manhunterOnTameFailChance>
      <trainability>None</trainability>

      <!-- Life stages cohérents avec la courbe d'âge -->
      <lifeStageAges>
        <li><def>HumanlikeBaby</def><minAge>0</minAge></li>
        <li><def>HumanlikeChild</def><minAge>3</minAge></li>
        <li><def>HumanlikeTeenager</def><minAge>13</minAge></li>
        <li><def>HumanlikeAdult</def><minAge>18</minAge></li>
      </lifeStageAges>
      
      <!-- Sons -->
      <soundMeleeHitPawn>Pawn_Melee_Punch_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_Punch_HitBuilding_Wood</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_Punch_Miss</soundMeleeMiss>
      <soundMeleeDodge>Pawn_MeleeDodge</soundMeleeDodge>
      
      <!-- Courbe d'âge cohérente avec life stages et lifeExpectancy -->
      <ageGenerationCurve>
        <points>
          <li>(0, 0)</li>
          <li>(18, 0)</li>    <!-- Correspond au début de HumanlikeAdult -->
          <li>(25, 100)</li>  <!-- Pic optimal -->
          <li>(100, 100)</li>
          <li>(180, 30)</li>  <!-- Diminue avant la mort -->
          <li>(200, 0)</li>   <!-- Correspond à lifeExpectancy -->
        </points>
      </ageGenerationCurve>
      
      <hediffGiverSets>
        <li>OrganicStandard</li>
        <li>Human</li>
      </hediffGiverSets>

      
    </race>

      <tools>
    <li>
      <label>poingsGauches</label>
      <capacities>
        <li>Blunt</li>
      </capacities>
      <power>18</power>
      <cooldownTime>1.5</cooldownTime>
      <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
    </li>
    <li>
      <label>poingsDroits</label>
      <capacities>
        <li>Blunt</li>
      </capacities>
      <power>18</power>
      <cooldownTime>1.5</cooldownTime>
      <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
    </li>
    <li>
      <label>morsure</label>
      <capacities>
        <li>Bite</li>
      </capacities>
      <power>25</power>
      <cooldownTime>2.0</cooldownTime>
      <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
      <chanceFactor>0.7</chanceFactor>
    </li>
  </tools>
  </ThingDef>

