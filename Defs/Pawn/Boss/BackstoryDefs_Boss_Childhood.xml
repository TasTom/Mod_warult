<?xml version="1.0" encoding="utf-8"?>
<Defs>

<!-- Classe de base pour les backstories Expedition 33 -->
<AlienRace.AlienBackstoryDef Name="Expedition33BackStory" Abstract="True">
  <disallowedTraitsChance>
    <li>
      <defName Degree="-1">Immunity</defName>
      <chance>100</chance>
    </li>
  </disallowedTraitsChance>
  <bodyTypeMale>Male</bodyTypeMale>
  <bodyTypeFemale>Female</bodyTypeFemale>
</AlienRace.AlienBackstoryDef>

<!-- ========================== CHILDHOOD ========================== -->

<!-- L'Évêque - Childhood -->
<AlienRace.AlienBackstoryDef ParentName="Expedition33BackStory">
  <defName>Expedition33_Eveque_Childhood</defName>
  <title>Enfant mystique</title>
  <titleShort>Mystique</titleShort>
  <baseDesc>{PAWN_nameDef} manifestait des pouvoirs divins dès son plus jeune âge. Les prêtres du village parlaient de miracles quand {PAWN_pronoun} était présent.</baseDesc>
  <slot>Childhood</slot>
  <!-- <bioAgeRange>
    <min>0</min>
    <max>16</max>
  </bioAgeRange> -->
  <skillGains>
    <Intellectual>4</Intellectual>
    <Social>2</Social>
  </skillGains>
  <spawnCategories>
    <li>Expedition33_DivineEntity</li>
  </spawnCategories>
</AlienRace.AlienBackstoryDef>

