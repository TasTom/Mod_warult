	<PawnKindDef>
		<defName>Expedition33_Expeditionnaire</defName>
		<label>expéditionnaire</label>
		<race>Human</race>
		<defaultFactionType>Expedition33</defaultFactionType>
		<combatPower>80</combatPower>
		<canArriveManhunter>false</canArriveManhunter>
		<canBeSapper>false</canBeSapper>
		<!-- SUPPRIME canBeGuard qui n'existe pas -->
		<lifeStages>
			<li>
				<bodyGraphicData>
					<texPath>Things/Pawn/Humanlike/Bodies/Naked_Male</texPath>
					<graphicClass>Graphic_Multi</graphicClass>
					<shaderType>CutoutComplex</shaderType>
				</bodyGraphicData>
			</li>
		</lifeStages>
		<weaponTags>
			<li>SimpleGun</li>
			<li>IndustrialGunAdvanced</li>
		</weaponTags>
		<apparelTags>
			<li>IndustrialBasic</li>
			<li>IndustrialAdvanced</li>
			<li>Military</li>
		</apparelTags>
		<initialResistanceRange>10~20</initialResistanceRange>
		<initialWillRange>1~3</initialWillRange>
		<backstoryCryptosleepCommonality>0.1</backstoryCryptosleepCommonality>
	</PawnKindDef>

