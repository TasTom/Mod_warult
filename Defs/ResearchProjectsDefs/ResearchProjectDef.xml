<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ResearchProjectDef>
        <defName>Expedition33_GommageAnalysis</defName>
        <label>Analyse du Gommage</label>
        <description>Étude approfondie du phénomène de Gommage pour comprendre ses mécanismes.</description>
        <baseCost>1000</baseCost>
        <techLevel>Industrial</techLevel>
        <tab>Expedition33_ResearchTab</tab> <!-- MODIFIÉ -->
        <researchViewX>1</researchViewX>
        <researchViewY>1</researchViewY>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>Expedition33_ArtisticDetection</defName>
        <label>Détection Artistique</label>
        <description>Développement de méthodes pour détecter la corruption artistique.</description>
        <baseCost>800</baseCost>
        <techLevel>Industrial</techLevel>
        <tab>Expedition33_ResearchTab</tab> <!-- MODIFIÉ -->
        <prerequisites>
            <li>Expedition33_GommageAnalysis</li>
        </prerequisites>
        <researchViewX>2</researchViewX>
        <researchViewY>1</researchViewY>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>Expedition33_BasicProtection</defName>
        <label>Protection Basique</label>
        <description>Premiers équipements de protection contre les effets mineurs du Gommage.</description>
        <baseCost>1200</baseCost>
        <techLevel>Industrial</techLevel>
        <tab>Expedition33_ResearchTab</tab> <!-- MODIFIÉ -->
