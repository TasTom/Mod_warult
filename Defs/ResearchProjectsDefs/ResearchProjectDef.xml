        <defName>Expedition33_CursedAgeDetector</defName>
        <label>Détecteur d'Âge Maudit</label>
        <description>Construction d'un appareil capable d'identifier l'âge actuellement peint.</description>
        <baseCost>1500</baseCost>
        <techLevel>Industrial</techLevel>
        <tab>Expedition33_ResearchTab</tab> <!-- MODIFIÉ -->
        <prerequisites>
            <li>Expedition33_BasicProtection</li>
        </prerequisites>
        <researchViewX>4</researchViewX>
        <researchViewY>0</researchViewY>
    </ResearchProjectDef>

    <ResearchProjectDef>
        <defName>Expedition33_AntiGommageShield</defName>
        <label>Bouclier Anti-Gommage</label>
